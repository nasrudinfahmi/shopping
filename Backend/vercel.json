{
  "version": 2,
  "builds": [
    {
      "src": "src/functions/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/functions/api.js"
    }
  ],
  "functions": {
    "path/to/dir/**/*": {
      "maxDuration": 50
    }
  }
}
